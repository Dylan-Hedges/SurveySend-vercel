{
  "name": "nodewithreactfullstack2019",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "postinstall": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && DISABLE_ESLINT_PLUGIN=true npm run build --prefix client",
    "webhook": "forever sendgrid_webhook.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dylan-Hedges/NodeWithReactFullStack2019.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dylan-Hedges/NodeWithReactFullStack2019/issues"
  },
  "homepage": "https://github.com/Dylan-Hedges/NodeWithReactFullStack2019#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "cookie-session": "^1.3.3",
    "express": "^4.17.1",
    "localtunnel": "^1.9.2",
    "lodash": "^4.17.15",
    "mongoose": "^5.6.1",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "path-parser": "^4.2.0",
    "sendgrid": "^5.2.3",
    "stripe": "^7.5.0"
  },
  "devDependencies": {
    "forever": "^1.0.0"
  }
}
